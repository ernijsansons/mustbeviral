{
  "name": "mustbeviral",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:frontend": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=__tests__/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --grep='@smoke'",
    "test:performance": "node scripts/performance-monitor.js",
    "deploy": "wrangler pages deploy dist",
    "deploy:staging": "node scripts/deploy-cloudflare.js staging",
    "deploy:production": "node scripts/deploy-cloudflare.js production",
    "deploy:workers": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy dist",
    "rollback:staging": "node scripts/rollback.js rollback staging",
    "rollback:production": "node scripts/rollback.js rollback production",
    "health:check": "node scripts/health-check.js check",
    "health:staging": "node scripts/health-check.js check staging",
    "health:production": "node scripts/health-check.js check production",
    "health:monitor": "node scripts/health-check.js monitor",
    "postinstall": "npm run setup:husky || true",
    "setup:husky": "command -v husky >/dev/null 2>&1 && husky || echo 'Husky not installed, skipping'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit && npm run typecheck",
    "security:fix": "npm audit fix",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:lighthouse": "lhci autorun",
    "perf:build-analyze": "npm run build:analyze && npm run perf:monitor",
    "ci:performance": "npm run build && npm run perf:monitor && npm run perf:lighthouse",
    "test:a11y": "playwright test --config=playwright-a11y.config.ts",
    "test:ui": "playwright test --ui",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "setup:report": "node scripts/setup-report.mjs"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit"
    ],
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250912.0",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@neondatabase/serverless": "^1.0.1",
    "@tanstack/react-query": "^5.90.2",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "eslint-config-next": "^15.5.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "glob": "^11.0.3",
    "google-trends-api": "^4.9.2",
    "isomorphic-dompurify": "^2.28.0",
    "jose": "^6.0.13",
    "lucide-react": "^0.344.0",
    "next": "^15.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.2",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "wouter": "^3.7.1",
    "wrangler": "^4.36.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^8.4.5",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-onboarding": "^8.4.5",
    "@storybook/addon-viewport": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-vite": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.10.2",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "chromatic": "^11.15.0",
    "commitizen": "^3.0.0",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.2.0",
    "msw": "^2.6.4",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.7",
    "vite-plugin-compression2": "^2.2.1"
  }
}
